{
  "name": "@photo/root",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "format:apps": "prettier --write apps/*/src",
    "format:packages": "prettier --write packages/*/src",
    "format:all": "yarn run format:apps && yarn run format:packages",
    "format:check": "prettier --check .",
    "lint": "eslint 'apps/dashboard/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint . --fix",
    "clean:all": "yarn cache clean && rm -rf node_modules apps/*/node_modules packages/*/node_modules yarn.lock",
    "re-install": "yarn run clean:all && yarn install",
    "start:common": "yarn workspace @packages/common run watch",
    "build:common": "yarn workspace @packages/common run build",
    "start:atoms": "yarn workspace @packages/atoms run watch",
    "start:molecules": "yarn workspace @packages/molecules run watch",
    "start:organisms": "yarn workspace @packages/organisms run watch",
    "start:service": "yarn workspace @photo/service start:development",
    "build:service": "yarn workspace @photo/service build",
    "build:dashboard": "yarn workspace @photo/dashboard build",
    "dev:dashboard": "concurrently \"yarn run start:service\" \"yarn run start:common\" \"yarn run start:atoms\" \"yarn run start:molecules\" \"yarn run start:organisms\" \"yarn workspace @photo/dashboard start\"",
    "start:dashboard": "yarn run start:service & yarn workspace @photo/dashboard serve",
    "start:web": "yarn workspace @photo/web dev",
    "dev:web": "concurrently \"yarn run start:service\" \"yarn run start:atoms\" \"yarn run start:web\"",
    "start:dev": "concurrently \"yarn run start:service\" \"yarn run start:atoms\" \"yarn run start:web\" \"yarn run start:dashboard\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@mui/x-data-grid": "^6.20.4",
    "@mui/x-date-pickers": "^6.10.0",
    "@mui/x-date-pickers-pro": "^6.10.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "formik": "^2.4.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@jest/console": "^29.7.0",
    "@jest/core": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.21",
    "@types/node": "^20.17.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/redux-mock-store": "^1.0.6",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "msw": "^2.2.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.4.2",
    "redux-mock-store": "^1.5.4",
    "rollup-plugin-visualizer": "^5.13.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.0.2",
    "vite": "^6.0.6",
    "vite-plugin-compression": "^0.5.1",
    "copyfiles": "^2.4.1"
  },
  "resolutions": {
    "axios": "1.4.0",
    "typescript": "5.0.2"
  },
  "engines": {
    "yarn": "1.22.22"
  },
  "packageManager": "yarn@1.22.22"
}
